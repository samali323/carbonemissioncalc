ck_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="183"/><column index="2" value="199"/><column index="3" value="123"/><column index="4" value="112"/><column index="5" value="215"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">WITH unique_match_emissions AS (
  SELECT
    ROW_NUMBER() OVER (PARTITION BY home_team, away_team ORDER BY id) AS rn,
    id, home_team, away_team, distance_km, flight_type, 
    total_emissions, per_passenger_emissions, 
    rail_emissions, bus_emissions, rail_saved, bus_saved, 
    carbon_price, carbon_cost_air, carbon_cost_rail, 
    carbon_cost_bus, flight_duration, driving_duration, 
    transit_duration, driving_distance, transit_distance, 
    last_updated
  FROM match_emissions
  WHERE (home_team, away_team) IN (
    SELECT home_team, away_team
    FROM routes
  )
)
INSERT OR REPLACE INTO match_emissions (
  id, home_team, away_team, distance_km, flight_type,
  total_emissions, per_passenger_emissions,
  rail_emissions, bus_emissions, rail_saved, bus_saved,
  carbon_price, carbon_cost_air, carbon_cost_rail,
  carbon_cost_bus, flight_duration, driving_duration,
  transit_duration, driving_distance, transit_distance,
  last_updated
)
SELECT
  id, home_team, away_team, distance_km, flight_type,
  total_emissions, per_passenger_emissions,
  rail_emissions, bus_emissions, rail_saved, bus_saved,
  carbon_price, carbon_cost_air, carbon_cost_rail,
  carbon_cost_bus, flight_duration, driving_duration,
  transit_duration, driving_distance, transit_distance,
  last_updated
FROM unique_match_emissions
WHERE rn = 1;</sql><current_tab id="0"/></tab_sql></sqlb_project>
